        </activity>
        <activity android:name=".mFragments.MedicineFragment" />
        <activity android:name=".mFragments.TreatmentFragment" />
        <activity android:name=".mFragments.AddMedicineFragment" />
        <activity android:name=".mFragments.SearchFragment" />
